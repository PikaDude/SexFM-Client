{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "SexFM Player",
  "version": "1.0.0",
  "identifier": "live.sexfm",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn generate"
  },
  "app": {
    "windows": [
      {
        "title": "SexFM Player",
        "width": 284,
        "height": 427,
        "resizable": false,
        "fullscreen": false,
        "decorations": false,
        "shadow": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
